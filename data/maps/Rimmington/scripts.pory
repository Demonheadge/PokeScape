    switch (var(VAR_RESULT))
    {
        case 0:
            msgbox(format("Going to your house."))
            //goto (SecretBase_EventScript_CheckEntrance_POH)            
        case 1:
            msgbox(format("Going to Cursed You's House."))
            warp (MAP_POH_1,0,0,0)
