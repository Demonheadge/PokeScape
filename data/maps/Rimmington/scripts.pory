script Rimmington_STATE{
    if ((var(VAR_POKESCAPE_GAMEMODE_STATE) == 1) || ((var(VAR_POKESCAPE_STORYMODE_PROGRESS) >= 110))) {
        setflag(FLAG_POKESCAPE_RIMMINGTON_HAM_LOST_TIME)
