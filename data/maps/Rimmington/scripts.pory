            setobjectxyperm(20,20,7)
			setobjectmovementtype(20,MOVEMENT_TYPE_WALK_IN_PLACE_LEFT)

            setobjectxyperm(15,23,7)
			setobjectmovementtype(15,MOVEMENT_TYPE_JOG_IN_PLACE_UP)

