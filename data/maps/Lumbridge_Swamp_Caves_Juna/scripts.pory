script JUNA_TOGGLE_TEMP_FLAG_1 {
    if (!flag(FLAG_TEMP_A))
    {
        setflag(FLAG_TEMP_A)
        return  
    }
    if (flag(FLAG_TEMP_A))
    {
        clearflag(FLAG_TEMP_A) 
        return 
    }
    return
}
