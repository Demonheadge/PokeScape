script JUNA_POOL_ALL_FLAGS{
    //State of the Tears
    if (flag(FLAG_TEMP_A)){
        if (flag(FLAG_TEMP_B)){
            if (flag(FLAG_TEMP_C)){
                if (flag(FLAG_TEMP_D)){
                    if (flag(FLAG_TEMP_E)){
                        if (flag(FLAG_TEMP_F)){
                            msgbox(format("BEEP BOOP."))
                            closemessage
                            setflag(FLAG_TEMP_10)
                            call(JUNA_POOL_DIRTY)
                            return
                        }
                        
                    }
                    
                }
                
            } 
            
        }
        
    }
    return
}


