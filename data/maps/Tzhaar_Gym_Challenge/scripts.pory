script CHALLENGE_START {
    setvar(VAR_TEMP_D, 1)
    applymovement(OBJ_EVENT_ID_PLAYER, ENTER_FIGHT_CAVES)
    waitmovement(0)
    if (!flag(FLAG_BADGE03_GET)) {
        setrespawn(HEAL_LOCATION_TZHAAR_GYM_ROOM)
        call(TZHAAR_GYM_CHALLENGE_WAVES_CONTROL)
    }
    else {
        call(FIGHT_CAVES_CHECK_IF_GOTO_NEXT_WAVE)
    }
    release
    end
}
movement ENTER_FIGHT_CAVES {
    walk_up * 5
    walk_right * 3
    walk_up * 2
    walk_right
    face_down
}

script FIGHTCAVES_WAVE_TRANSITION_START {
    //fadescreenswapbuffers
    fadescreenswapbuffers(FADE_TO_BLACK)
    buffernumberstring(0, VAR_TEMP_E)
    msgbox(format("WAVE: {STR_VAR_1}"))
	closemessage
    //fadescreenswapbuffers(FADE_FROM_BLACK)
    return
}

script FIGHTCAVES_WAVE_TRANSITION_END {
    fadescreenswapbuffers(FADE_FROM_BLACK)
    return
}

