script mathtest {
    setvar(VAR_TEMP_A, 30) //base level.
    setvar(VAR_TEMP_B, 6)  //amount of randomness
    random(VAR_TEMP_B) 
    copyvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_RESULT) //Copy it out of TEMP_A to be used elsewhere.
    addvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_TEMP_A) // Add A + B(result) = The intended Level. into TEMP_A.
    buffernumberstring(0, VAR_CUSTOM_TRAINERPOOL_LEVEL)
    msgbox(format("VAR_CUSTOM_TRAINERPOOL_LEVEL: {STR_VAR_1}"))
	closemessage
}


script WAVE_LEVELS { //This sets the levels of the monsters of each wave.
    switch(var(VAR_TEMP_E)){
        case 0:
        case 1:
        case 2:
        case 3:
        case 4:
        case 5:
        case 6:
        case 7:
        case 8:
        case 9:
		case 10: // 30 - 35
            setvar(VAR_TEMP_A, 30) //base level.
            setvar(VAR_TEMP_B, 6)  //amount of randomness 0,1,2,3,4,5
            random(VAR_TEMP_B) 
            copyvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_RESULT) //Copy it out of TEMP_A to be used elsewhere.
            addvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_TEMP_A) // Add A + B(result) = The intended Level. into TEMP_A.
        case 11:
        case 12:
        case 13:
        case 14:
        case 15: // 35 - 40
            setvar(VAR_TEMP_A, 35) //base level.
            setvar(VAR_TEMP_B, 6)  //amount of randomness 0,1,2,3,4,5
            random(VAR_TEMP_B) 
            copyvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_RESULT) //Copy it out of TEMP_A to be used elsewhere.
            addvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_TEMP_A) // Add A + B(result) = The intended Level. into TEMP_A.
        case 16:
        case 17:
        case 18:
        case 19:
        case 20: // 40 - 50
            setvar(VAR_TEMP_A, 40) //base level.
            setvar(VAR_TEMP_B, 11)  //amount of randomness 0,1,2,3,4,5
            random(VAR_TEMP_B) 
            copyvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_RESULT) //Copy it out of TEMP_A to be used elsewhere.
            addvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_TEMP_A) // Add A + B(result) = The intended Level. into TEMP_A.
        case 21:
        case 22:
        case 23:
        case 24:
        case 25: // 50 - 60
            setvar(VAR_TEMP_A, 50) //base level.
            setvar(VAR_TEMP_B, 11)  //amount of randomness 0,1,2,3,4,5
            random(VAR_TEMP_B) 
            copyvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_RESULT) //Copy it out of TEMP_A to be used elsewhere.
            addvar(VAR_CUSTOM_TRAINERPOOL_LEVEL, VAR_TEMP_A) // Add A + B(result) = The intended Level. into TEMP_A.
    }
    return
}




