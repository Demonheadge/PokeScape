script ENCOUNTER_TZ_KIH {
    lock
    faceplayer
    namebox("Tz-Kih")
    playmoncry(SPECIES_TZ_KIH, 0)
	msgbox(format("Keehsse!"))
    waitmoncry
    hidenamebox
	closemessage
	delay(1)
    specialvar (VAR_RESULT, GetCurrentMap)
    if (var(VAR_RESULT) == MAP_TZHAAR_CAVES_5) {
        setwildbattle (SPECIES_TZ_KIH, 30, ITEM_NONE)
    }
    else { // if fightcaves
        call(WAVE_LEVELS)
        setwildbattle (SPECIES_TZ_KIH, VAR_CUSTOM_TRAINERPOOL_LEVEL, ITEM_FIRECAPE) 
    }
	dowildbattle
    switch(var(VAR_RESULT)){
		case B_OUTCOME_WON:
        case B_OUTCOME_CAUGHT:
            specialvar (VAR_RESULT, GetCurrentMap)
            if (var(VAR_RESULT) == MAP_TZHAAR_CAVES_5) {
                fadescreenswapbuffers(FADE_TO_BLACK)
                removeobject (VAR_LAST_TALKED)
                fadescreenswapbuffers(FADE_FROM_BLACK)
                release
                end
            }
            return // if fightcaves
    }
    release
	end
}
