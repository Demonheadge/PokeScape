	call HIDDEN_GROTTO_LEVELS
	msgbox ENCOUNTER_RANDOM_Text_0
	playmoncry SPECIES_LUGIA, 0
	msgbox ENCOUNTER_RANDOM_Text_1
	waitmoncry
	hidenamebox
	closemessage
	delay 1
	compare VAR_STORE_GROTTO_SEED_RESULT, 44
	goto_if_le ENCOUNTER_RANDOM_2
	compare VAR_STORE_GROTTO_SEED_RESULT, 79
	goto_if_le ENCOUNTER_RANDOM_3
	compare VAR_STORE_GROTTO_SEED_RESULT, 94
	goto_if_le ENCOUNTER_RANDOM_4
	compare VAR_STORE_GROTTO_SEED_RESULT, 99
	goto_if_le ENCOUNTER_RANDOM_5
	compare VAR_STORE_GROTTO_SEED_RESULT, 100
	goto_if_le ENCOUNTER_RANDOM_6
ENCOUNTER_RANDOM_1:
	dowildbattle
	compare VAR_RESULT, B_OUTCOME_WON
	goto_if_eq ENCOUNTER_RANDOM_13
	compare VAR_RESULT, B_OUTCOME_CAUGHT
	goto_if_eq ENCOUNTER_RANDOM_14
	compare VAR_RESULT, B_OUTCOME_RAN
	goto_if_eq ENCOUNTER_RANDOM_15
ENCOUNTER_RANDOM_12:
	call RandomiseSeed_Grotto
	return

ENCOUNTER_RANDOM_2:
	call RANDOM_DOWILDBATTLE_MON_COMMON
	goto ENCOUNTER_RANDOM_1

ENCOUNTER_RANDOM_3:
