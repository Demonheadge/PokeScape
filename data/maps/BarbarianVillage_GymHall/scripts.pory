script BarbarianVillage_GYM_RESTART 
{
    if (var(VAR_POKESCAPE_BARBARIAN_VILLAGE_STATE) == 2)
    {
        setvar(VAR_POKESCAPE_BARBARIAN_VILLAGE_STATE, 1)
    }
    return
}


