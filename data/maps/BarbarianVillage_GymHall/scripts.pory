    msgbox(format("And the winner is…{PAUSE 15}…{PAUSE 15}{PLAYER}!"))
    hidenamebox
    closemessage
    playse (SE_M_ENCORE2)

    call (ContestHall_EventScript_VObjectAudienceLookAround)

    applymovement(OBJ_EVENT_ID_PLAYER, BARBARIAN_GYM_MOVEMENT_10)
