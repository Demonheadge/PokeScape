    waitmovement(0)
    playse (SE_M_ENCORE2)
    
    call (ContestHall_EventScript_VObjectAudienceLookAround)

    applymovement(OBJ_EVENT_ID_HAAKON, MOVEMENT_FACE_DOWN)
